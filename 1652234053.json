, n);\r\n}\r\n \r\nint main()\r\n{\r\n  int x[] = { 1, 10, 9,  7, 3, 0 };\r\n  int i;\r\n  int len = sizeof(x)/sizeof(x[0]);\r\n  printf(\"Original Array:\\n\");\r\n  for (i = 0; i < len; i++)\r\n  printf(\"%d%s\", x[i], i == len - 1 ? \"\\n\" : \" \");  \r\n  printf(\"\\nSorted Array:\\n\");\r\n  bogosort(x, 6);\r\n  for (i=0; i < 6; i++) printf(\"%d \", x[i]);\r\n  printf(\"\\n\");\r\n}\r\n"}}, "framework": {"brandingInformation": {"homepageUrl": "https://github.com/mull-project/mull"}, "name": "Mull", "version": "0.17.1, LLVM 12.0.0"}, "mutationScore": 29, "schemaVersion": "1.7", "thresholds": {"high": 80, "low": 60}}